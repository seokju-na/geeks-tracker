{
  "name": "geeks-tracker",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "tauri dev",
    "dev:webview": "vite",
    "build": "tauri build",
    "build:webview": "tsc && vite build",
    "test": "yarn test:tauri && yarn test:webview",
    "test:tauri": "cargo test --manifest-path ./src-tauri/Cargo.toml --workspace",
    "test:webview": "jest",
    "postinstall": "husky install",
    "lint": "eslint --cache -c .eslintrc.js 'src/**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit --incremental"
  },
  "engines": {
    "node": ">=16.14.1 <17.0.0",
    "yarn": "^3.2.0"
  },
  "packageManager": "yarn@3.2.0",
  "dependencies": {
    "@stitches/react": "^1.2.6",
    "@tauri-apps/api": "^1.0.0-rc.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.0.0-rc.5",
    "@types/eslint": "^8",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-react": "^1.0.7",
    "esbuild": "^0.14.27",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.4",
    "vite": "^2.8.0"
  }
}
